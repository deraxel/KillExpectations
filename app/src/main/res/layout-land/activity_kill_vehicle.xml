        android:fontFamily="sans-serif-medium"
        android:textSize="20sp"
        android:textAllCaps="true"
        android:layout_below="@+id/ResetEQ"
        android:id="@+id/button3"
        android:layout_marginTop="5sp"
        android:layout_alignLeft="@id/ResetEQ"
        android:layout_alignStart="@id/ResetEQ"
        android:layout_alignRight="@id/ResetEQ"
        android:layout_alignEnd="@id/ResetEQ" />

