        style="@android:style/Widget.DeviceDefault.Button"
        android:visibility="invisible"
        android:textAlignment="center"
        android:fontFamily="sans-serif-medium"
        android:textSize="20sp"
        android:textAllCaps="false"
        android:textStyle="bold"
        android:textColor="@color/buttontextColor"
        android:background="@color/buttonColor"
        android:onClick="cus4D"
        android:id="@+id/cus4D"/>

    <Button
        android:text="@string/reset"
        android:layout_width="100sp"
        android:layout_height="40sp"
        android:id="@+id/ResetEQ"
        style="@android:style/Widget.DeviceDefault.Button"
        android:visibility="visible"
        android:textAlignment="center"
        android:fontFamily="sans-serif-medium"
        android:textSize="20sp"
        android:textAllCaps="true"
        android:textStyle="bold"
        android:textColor="@color/buttontextColor"
        android:background="@color/buttonColor"
        android:layout_below="@id/gotoTro"
        android:layout_alignLeft="@id/resetloc"
        android:layout_alignStart="@id/resetloc"/>

    <CheckBox
        android:text="@string/twin_linked"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/explodeexptxt"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/twin_linked"
        android:checked="false"
        android:textSize="12sp"
        android:textColor="@color/chkbx"
        app:buttonTint="@color/chkbx" />

    <CheckBox
        android:text="@string/rending"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/rending"
        android:checked="false"
        android:textSize="12sp"
        android:layout_below="@+id/twin_linked"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="@color/chkbx"
        app:buttonTint="@color/chkbx" />

    <CheckBox
        android:text="@string/preferred_enemy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Pref_Enemy"
        android:checked="false"
        android:textSize="12sp"
        android:layout_toRightOf="@id/ordinance"
        android:layout_toEndOf="@id/ordinance"
        android:layout_below="@+id/explodeexptxt"
        android:textColor="@color/chkbx"
        app:buttonTint="@color/chkbx" />

    <CheckBox
        android:text="@string/armorbane"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/melta"
        android:checked="false"
        android:textSize="12sp"
        android:layout_alignLeft="@+id/Pref_Enemy"
        android:layout_alignStart="@+id/Pref_Enemy"
        android:layout_below="@+id/Pref_Enemy"
        android:textColor="@color/chkbx"
        app:buttonTint="@color/chkbx" />

    <CheckBox
        android:text="@string/ordinance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ordinance"
        android:checked="false"
        android:textSize="12sp"
        android:layout_below="@+id/explodeexpoutp"
        android:layout_toRightOf="@id/twin_linked"
        android:layout_toEndOf="@id/twin_linked"
        android:textColor="@color/chkbx"
        app:buttonTint="@color/chkbx" />

    <Button
        android:text="@string/uparrow"
        android:textSize="12sp"
        android:layout_width="40sp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/ap"
        android:layout_alignLeft="@+id/ap"
        android:layout_alignTop="@id/rate_of_fire"
        android:layout_alignBottom="@id/rate_of_fire"
        android:background="@color/arrowbuttons"
        android:id="@+id/rofUP"
        android:textColor="@color/buttontextColor"
        android:textAlignment="center" />

    <Button
        android:text="@string/downarrow"
        android:textSize="12sp"
        android:layout_width="40sp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/rate_of_fire"
        android:layout_alignBottom="@id/rate_of_fire"
        android:layout_toLeftOf="@id/rlbox"
        android:layout_toStartOf="@id/rlbox"
        android:background="@color/arrowbuttons"
        android:id="@+id/rofDOWN"
        android:textColor="@color/buttontextColor"
        android:textAlignment="center" />

    <EditText
        android:layout_width="wrap_content"
        android:inputType="number"
        android:id="@+id/rof"
        android:text="@string/_0"
        android:background="@color/buttonColor"
        android:layout_alignStart="@+id/rofUP"
        android:layout_alignLeft="@+id/rofUP"
        android:layout_alignEnd="@id/rofDOWN"
        android:layout_alignRight="@id/rofDOWN"
        android:layout_alignTop="@+id/rate_of_fire"
        android:layout_alignBottom="@+id/rate_of_fire"
        android:textSize="12sp"
        android:textAlignment="center"
        android:layout_height="wrap_content" />


</RelativeLayout>
