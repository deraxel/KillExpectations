<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_kill_troops"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backgroundColor"
    tools:context="com.killpredictions.killexpectations.KillTroops">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="200sp"
        android:layout_alignEnd="@+id/center_line"
        android:layout_alignRight="@+id/center_line"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:id="@+id/offensive_background"
        android:background="@color/offcolors"
        android:visibility="visible" >

        <TextView
            android:text="@string/offensive_presets"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/Defnoimputtext2"
            android:textColor="@color/presettxtcolors"
            android:textAlignment="center"/>
    </FrameLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="200sp"
        android:layout_alignStart="@+id/center_line"
        android:layout_alignLeft="@+id/center_line"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:id="@+id/defensive_background"
        android:background="@color/defcolors"
        android:visibility="visible" >

        <TextView
            android:text="@string/defensive_presets"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/Defnoimputtext"
            android:textColor="@color/presettxtcolors"
            android:textAlignment="center"/>

    </FrameLayout>

    <RelativeLayout
        android:layout_width="0sp"
        android:layout_height="110sp"
        android:id="@+id/mtl"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:visibility="invisible"/>

    <RelativeLayout
        android:layout_width="0sp"
        android:layout_height="165sp"
        android:id="@+id/ttl"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:visibility="invisible"/>


    <Button
        android:textSize="18sp"
        android:layout_width="20sp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:background="@color/gotoveh"
        android:id="@+id/gotoVeh"
        android:textAlignment="center"
        android:textColor="@color/gototext"
        android:text="@string/kills_vs_vehicles" />

    <RelativeLayout
        android:layout_width="0sp"
        android:layout_height="0sp"
        android:id="@+id/center_line"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:visibility="invisible"/>

    <RelativeLayout
        android:layout_width="82sp"
        android:layout_height="0sp"
        android:id="@+id/rdl"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_centerVertical="true"
        android:visibility="invisible"/>

    <RelativeLayout
        android:layout_width="82sp"
        android:layout_height="0sp"
        android:id="@+id/ldl"
        android:layout_alignLeft="@id/center_line"
        android:layout_alignStart="@id/center_line"
        android:layout_centerVertical="true"
        android:visibility="invisible"/>

    <RelativeLayout
        android:layout_width="82sp"
        android:layout_height="0sp"
        android:id="@+id/lol"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true"
        android:visibility="invisible"/>

    <RelativeLayout
        android:layout_width="82sp"
        android:layout_height="0sp"
        android:id="@+id/rol"
        android:layout_alignRight="@id/center_line"
        android:layout_alignEnd="@id/center_line"
        android:layout_centerVertical="true"
        android:visibility="invisible"/>

    <RelativeLayout
        android:layout_width="0sp"
        android:layout_height="55sp"
        android:id="@+id/btl"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:visibility="invisible"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/ballistic_skill"
        android:textColor="@color/attrtext"
        android:textSize="20sp"
        android:id="@+id/ballistic_skill"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_below="@id/gotoVeh"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/strength"
        android:textColor="@color/attrtext"
        android:textSize="20sp"
        android:layout_below="@+id/ballistic_skill"
        android:id="@+id/strength" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/ap"
        android:textColor="@color/attrtext"
        android:textSize="20sp"
        android:id="@+id/armor_pen"
        android:layout_below="@+id/strength" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/number_of_shots"
        android:textColor="@color/attrtext"
        android:textSize="20sp"
        android:id="@+id/rate_of_fire"
        android:layout_below="@+id/armor_pen" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/target_toughness"
        android:textColor="@color/attrtext"
        android:textSize="20sp"
        android:id="@+id/toughness"
        android:layout_below="@+id/rate_of_fire" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/target_armor_save"
        android:textColor="@color/attrtext"
        android:textSize="20sp"
        android:id="@+id/armor_save"
        android:layout_below="@+id/toughness" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/target_invuln_save"
        android:textColor="@color/attrtext"
        android:textSize="20sp"
        android:id="@+id/invulnerable_save"
        android:layout_below="@+id/armor_save" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/target_feel_no_pain"
        android:textColor="@color/attrtext"
        android:textSize="20sp"
        android:id="@+id/feel_no_pain"
        android:layout_below="@+id/invulnerable_save" />

    <Spinner
        android:layout_width="140sp"
        android:id="@+id/BS"
        android:spinnerMode="dropdown"
        android:dropDownWidth="wrap_content"
        android:layout_alignTop="@+id/ballistic_skill"
        android:layout_alignBottom="@+id/ballistic_skill"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_height="wrap_content"
        android:background="@color/buttonColor" />

    <Spinner
        android:layout_width="140sp"
        android:id="@+id/str"
        android:spinnerMode="dropdown"
        android:dropDownWidth="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/strength"
        android:layout_alignBottom="@+id/strength"
        android:layout_height="wrap_content"
        android:background="@color/buttonColor" />

    <Spinner
        android:layout_width="140sp"
        android:id="@+id/ap"
        android:spinnerMode="dropdown"
        android:dropDownWidth="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/armor_pen"
        android:layout_alignBottom="@+id/armor_pen"
        android:layout_height="wrap_content"
        android:background="@color/buttonColor" />

    <Spinner
        android:layout_width="140sp"
        android:id="@+id/tgh"
        android:spinnerMode="dropdown"
        android:dropDownWidth="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/toughness"
        android:layout_alignBottom="@+id/toughness"
        android:layout_height="wrap_content"
        android:background="@color/buttonColor" />

    <Spinner
        android:layout_width="140sp"
        android:id="@+id/as"
        android:spinnerMode="dropdown"
        android:dropDownWidth="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/armor_save"
        android:layout_alignBottom="@+id/armor_save"
        android:layout_height="wrap_content"
        android:background="@color/buttonColor" />

    <Spinner
        android:layout_width="140sp"
        android:id="@+id/is"
        android:spinnerMode="dropdown"
        android:dropDownWidth="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/invulnerable_save"
        android:layout_alignBottom="@+id/invulnerable_save"
        android:layout_height="wrap_content"
        android:background="@color/buttonColor" />

    <Spinner
        android:layout_width="140sp"
        android:id="@+id/fnp"
        android:spinnerMode="dropdown"
        android:dropDownWidth="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/feel_no_pain"
        android:layout_alignBottom="@+id/feel_no_pain"
        android:layout_height="wrap_content"
        android:background="@color/buttonColor" />



    <TextView
        android:text="@string/expected_kills"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/e_ks"
        android:textSize="20sp"
        android:textColor="@color/exptextColor"
        android:textStyle="normal|bold"
        android:textAlignment="viewStart"
        android:layout_gravity="start"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/fnp"
        android:background="@color/expbackgroundColor"/>

    <TextView
        android:text="@string/expectedkill"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/expectedkill"
        android:textSize="20sp"
        android:textColor="@color/exptextColor"
        android:textStyle="normal|bold"
        android:textAlignment="center"
        android:layout_below="@+id/fnp"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"/>

    <Button
        android:text="@string/meq"
        android:layout_width="75sp"
        android:layout_height="wrap_content"
        android:id="@+id/MEQ"
        android:layout_alignTop="@id/btl"
        android:layout_alignLeft="@id/rdl"
        android:layout_alignStart="@id/rdl"
        style="@android:style/Widget.DeviceDefault.Button"
        android:visibility="visible"
        android:textAlignment="center"
        android:fontFamily="sans-serif-medium"
        android:textSize="20sp"
        android:textAllCaps="false"
        android:textStyle="bold"
        android:textColor="@color/buttontextColor"
        android:background="@color/buttonColor" />

    <Button
        android:text="@string/beq"
        android:layout_width="75sp"
        android:layout_height="wrap_content"
        android:id="@+id/BEQ"
        android:layout_alignTop="@id/btl"
        android:layout_alignLeft="@id/rol"
        android:layout_alignStart="@id/rol"
        style="@android:style/Widget.DeviceDefault.Button"
        android:visibility="visible"
        android:textAlignment="center"
        android:fontFamily="sans-serif-medium"
        android:textSize="20sp"
        android:textAllCaps="false"
        android:textStyle="bold"
        android:textColor="@color/buttontextColor"
        android:background="@color/buttonColor" />

    <Button
        android:text="@string/teq"
        android:layout_width="75sp"
        android:layout_height="wrap_content"
        android:id="@+id/TEQ"
        android:layout_alignTop="@id/btl"
        android:layout_alignRight="@id/ldl"
        android:layout_alignEnd="@id/ldl"
        style="@android:style/Widget.DeviceDefault.Button"
        android:visibility="visible"
        android:textAlignment="center"
        android:fontFamily="sans-serif-medium"
        android:textSize="20sp"
        android:textAllCaps="false"
        android:textStyle="bold"
        android:textColor="@color/buttontextColor"
        android:background="@color/buttonColor" />

    <Button
        android:text="@string/leq"
        android:layout_width="75sp"
        android:layout_height="wrap_content"
        android:id="@+id/LEQ"
        android:layout_alignTop="@id/btl"
        android:layout_alignRight="@id/lol"
        android:layout_alignEnd="@id/lol"
        style="@android:style/Widget.DeviceDefault.Button"
        android:visibility="visible"
        android:textAlignment="center"
        android:fontFamily="sans-serif-medium"
        android:textSize="20sp"
        android:textAllCaps="false"
        android:textStyle="bold"
        android:textColor="@color/buttontextColor"
        android:background="@color/buttonColor" />

    <Button
        android:text="@string/reset"
        android:layout_width="wrap_content"
        android:layout_height="40sp"
        android:id="@+id/ResetEQ"
        style="@android:style/Widget.DeviceDefault.Button"
        android:visibility="visible"
        android:textAlignment="center"
        android:fontFamily="sans-serif-medium"
        android:textSize="20sp"
        android:textAllCaps="true"
        android:textStyle="bold"
        android:textColor="@color/buttontextColor"
        android:background="@color/buttonColor"
        android:layout_below="@+id/expectedkill"
        android:layout_centerHorizontal="true" />

    <CheckBox
        android:text="@string/twin_linked"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/e_ks"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/twin_linked"
        android:checked="false"
        android:textSize="12sp"
        android:textColor="@color/chkbx"
        app:buttonTint="@color/chkbx"
        android:buttonTint="@color/chkbx"/>

    <CheckBox
        android:text="@string/rending"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/rending"
        android:checked="false"
        android:textSize="12sp"
        android:textColor="@color/chkbx"
        android:layout_below="@+id/twin_linked"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        app:buttonTint="@color/chkbx" />

    <CheckBox
        android:text="@string/preferred_enemy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Pref_Enemy"
        android:checked="false"
        android:textSize="12sp"
        android:textColor="@color/chkbx"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/expectedkill"
        app:buttonTint="@color/chkbx" />

    <CheckBox
        android:text="@string/shred"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Shred"
        android:checked="false"
        android:textSize="12sp"
        android:textColor="@color/chkbx"
        android:layout_alignLeft="@+id/Pref_Enemy"
        android:layout_alignStart="@+id/Pref_Enemy"
        android:layout_below="@+id/Pref_Enemy"
        app:buttonTint="@color/chkbx" />

    <CheckBox
        android:text="@string/instant_death"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/instaDeath"
        android:checked="false"
        android:textSize="12sp"
        android:textColor="@color/chkbx"
        android:layout_below="@+id/ResetEQ"
        android:layout_centerHorizontal="true"
        app:buttonTint="@color/chkbx" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="40sp"
        android:text="@string/graph"
        android:onClick="gotoGraph"
        android:textStyle="bold"
        android:textColor="@color/buttontextColor"
        android:background="@color/buttonColor"
        android:textAlignment="center"
        android:fontFamily="sans-serif-medium"
        android:textSize="20sp"
        android:textAllCaps="true"
        android:layout_below="@+id/instaDeath"
        android:layout_centerHorizontal="true"/>

    <Button
        android:text="@string/uparrow"
        android:textSize="12sp"
        android:layout_width="40sp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/fnp"
        android:layout_alignLeft="@+id/fnp"
        android:layout_alignTop="@id/rate_of_fire"
        android:layout_alignBottom="@id/rate_of_fire"
        android:background="@color/arrowbuttons"
        android:id="@+id/rofUP"
        android:textColor="@color/buttontextColor"
        android:textAlignment="center" />

    <Button
        android:text="@string/downarrow"
        android:textSize="12sp"
        android:layout_width="40sp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/rate_of_fire"
        android:layout_alignBottom="@id/rate_of_fire"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:background="@color/arrowbuttons"
        android:id="@+id/rofDOWN"
        android:textColor="@color/buttontextColor"
        android:textAlignment="center" />

    <Button
        android:text="@string/setsave"
        android:layout_width="75sp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/ttl"
        android:layout_alignRight="@id/lol"
        android:layout_alignEnd="@id/lol"
        style="@android:style/Widget.DeviceDefault.Button"
        android:visibility="visible"
        android:textAlignment="center"
        android:fontFamily="sans-serif-medium"
        android:textSize="20sp"
        android:textAllCaps="false"
        android:textStyle="bold"
        android:textColor="@color/buttontextColor"
        android:background="@color/buttonColor"
        android:onClick="setsav1O"
        android:id="@+id/setsav1O"/>

    <Button
        android:text="@string/cus1"
        android:layout_width="75sp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/ttl"
        android:layout_alignRight="@id/lol"
        android:layout_alignEnd="@id/lol"
        style="@android:style/Widget.DeviceDefault.Button"
        android:visibility="invisible"
        android:textAlignment="center"
        android:fontFamily="sans-serif-medium"
        android:textSize="20sp"
        android:textAllCaps="false"
        android:textStyle="bold"
        android:textColor="@color/buttontextColor"
        android:background="@color/buttonColor"
        android:onClick="cus1O"
        android:id="@+id/cus1O"/>

    <Button
        android:text="@string/setsave"
        android:layout_width="75sp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/ttl"
        android:layout_alignLeft="@id/rol"
        android:layout_alignStart="@id/rol"
        style="@android:style/Widget.DeviceDefault.Button"
        android:visibility="visible"
        android:textAlignment="center"
        android:fontFamily="sans-serif-medium"
        android:textSize="20sp"
        android:textAllCaps="false"
        android:textStyle="bold"
        android:textColor="@color/buttontextColor"
        android:background="@color/buttonColor"
        android:onClick="setsav2O"
        android:id="@+id/setsav2O"/>

    <Button
        android:text="@string/cus2"
        android:layout_width="75sp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/ttl"
        android:layout_alignLeft="@id/rol"
        android:layout_alignStart="@id/rol"
        style="@android:style/Widget.DeviceDefault.Button"
        android:visibility="invisible"
        android:textAlignment="center"
        android:fontFamily="sans-serif-medium"
        android:textSize="20sp"
        android:textAllCaps="false"
        android:textStyle="bold"
        android:textColor="@color/buttontextColor"
        android:background="@color/buttonColor"
        android:onClick="cus2O"
        android:id="@+id/cus2O"/>

    <Button
        android:text="@string/setsave"
        android:layout_width="75sp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/mtl"
        android:layout_alignRight="@id/lol"
        android:layout_alignEnd="@id/lol"
        style="@android:style/Widget.DeviceDefault.Button"
        android:visibility="visible"
        android:textAlignment="center"
        android:fontFamily="sans-serif-medium"
        android:textSize="20sp"
        android:textAllCaps="false"
        android:textStyle="bold"
        android:textColor="@color/buttontextColor"
        android:background="@color/buttonColor"
        android:onClick="setsav3O"
        android:id="@+id/setsav3O"/>

    <Button
        android:text="@string/cus3"
        android:layout_width="75sp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/mtl"
        android:layout_alignRight="@id/lol"
        android:layout_alignEnd="@id/lol"
        style="@android:style/Widget.DeviceDefault.Button"
        android:visibility="invisible"
        android:textAlignment="center"
        android:fontFamily="sans-serif-medium"
        android:textSize="20sp"
        android:textAllCaps="false"
        android:textStyle="bold"
        android:textColor="@color/buttontextColor"
        android:background="@color/buttonColor"
        android:onClick="cus3O"
        android:id="@+id/cus3O"/>


    <Button
        android:text="@string/setsave"
        android:layout_width="75sp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/mtl"
        android:layout_alignLeft="@id/rol"
        android:layout_alignStart="@id/rol"
        style="@android:style/Widget.DeviceDefault.Button"
        android:visibility="visible"
        android:textAlignment="center"
        android:fontFamily="sans-serif-medium"
        android:textSize="20sp"
        android:textAllCaps="false"
        android:textStyle="bold"
        android:textColor="@color/buttontextColor"
        android:background="@color/buttonColor"
        android:onClick="setsav4O"
        android:id="@+id/setsav4O"/>

    <Button
        android:text="@string/cus4"
        android:layout_width="75sp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/mtl"
        android:layout_alignLeft="@id/rol"
        android:layout_alignStart="@id/rol"
        style="@android:style/Widget.DeviceDefault.Button"
        android:visibility="invisible"
        android:textAlignment="center"
        android:fontFamily="sans-serif-medium"
        android:textSize="20sp"
        android:textAllCaps="false"
        android:textStyle="bold"
        android:textColor="@color/buttontextColor"
        android:background="@color/buttonColor"
        android:onClick="cus4O"
        android:id="@+id/cus4O"/>

    <Button
        android:text="@string/setsave"
        android:layout_width="75sp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/ttl"
        android:layout_alignRight="@id/ldl"
        android:layout_alignEnd="@id/ldl"
        style="@android:style/Widget.DeviceDefault.Button"
        android:visibility="visible"
        android:textAlignment="center"
        android:fontFamily="sans-serif-medium"
        android:textSize="20sp"
        android:textAllCaps="false"
        android:textStyle="bold"
        android:textColor="@color/buttontextColor"
        android:background="@color/buttonColor"
        android:onClick="setsav1D"
        android:id="@+id/setsav1D"/>

    <Button
        android:text="@string/cus1"
        android:layout_width="75sp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/ttl"
        android:layout_alignRight="@id/ldl"
        android:layout_alignEnd="@id/ldl"
        style="@android:style/Widget.DeviceDefault.Button"
        android:visibility="invisible"
        android:textAlignment="center"
        android:fontFamily="sans-serif-medium"
        android:textSize="20sp"
        android:textAllCaps="false"
        android:textStyle="bold"
        android:textColor="@color/buttontextColor"
        android:background="@color/buttonColor"
        android:onClick="cus1D"
        android:id="@+id/cus1D"/>

    <Button
        android:text="@string/setsave"
        android:layout_width="75sp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/ttl"
        android:layout_alignLeft="@id/rdl"
